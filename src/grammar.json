{
  "name": "smarty",
  "rules": {
    "template": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_smarty"
      }
    },
    "_smarty": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "inline"
        },
        {
          "type": "SYMBOL",
          "name": "include"
        },
        {
          "type": "SYMBOL",
          "name": "block"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "foreach"
        },
        {
          "type": "SYMBOL",
          "name": "if"
        },
        {
          "type": "SYMBOL",
          "name": "nocache"
        }
      ]
    },
    "_nested": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "inline"
        },
        {
          "type": "SYMBOL",
          "name": "include"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "foreach"
        },
        {
          "type": "SYMBOL",
          "name": "if"
        },
        {
          "type": "SYMBOL",
          "name": "nocache"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{*"
        },
        {
          "type": "PATTERN",
          "value": "[^*]*"
        },
        {
          "type": "STRING",
          "value": "*}"
        }
      ]
    },
    "inline": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "text"
          },
          "named": true,
          "value": "php"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "modifier"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "include": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{include"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{block"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_nested"
            }
          },
          "named": true,
          "value": "body"
        },
        {
          "type": "STRING",
          "value": "{/block}"
        }
      ]
    },
    "foreach": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{foreach"
        },
        {
          "type": "PATTERN",
          "value": "\\$[^\\s]+"
        },
        {
          "type": "STRING",
          "value": "as"
        },
        {
          "type": "PATTERN",
          "value": "\\$[^\\s=}]+"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "=>"
                },
                {
                  "type": "PATTERN",
                  "value": "\\$[^}]+"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "FIELD",
          "name": "alternative",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "foreach_else"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "{/foreach}"
        }
      ]
    },
    "foreach_else": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{foreachelse}"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_nested"
            }
          },
          "named": true,
          "value": "body"
        }
      ]
    },
    "if": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{if"
        },
        {
          "type": "FIELD",
          "name": "condition",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^}]+"
            },
            "named": true,
            "value": "text"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "FIELD",
            "name": "alternative",
            "content": {
              "type": "SYMBOL",
              "name": "else_if"
            }
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "FIELD",
              "name": "alternative",
              "content": {
                "type": "SYMBOL",
                "name": "else"
              }
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "{/if}"
        }
      ]
    },
    "else_if": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{elseif"
        },
        {
          "type": "FIELD",
          "name": "condition",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "PATTERN",
              "value": "[^}]+"
            },
            "named": true,
            "value": "text"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        }
      ]
    },
    "else": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{else}"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        }
      ]
    },
    "nocache": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{nocache}"
        },
        {
          "type": "FIELD",
          "name": "body",
          "content": {
            "type": "ALIAS",
            "content": {
              "type": "REPEAT",
              "content": {
                "type": "SYMBOL",
                "name": "_nested"
              }
            },
            "named": true,
            "value": "body"
          }
        },
        {
          "type": "STRING",
          "value": "{/nocache}"
        }
      ]
    },
    "modifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "[^|:}]+"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ":"
              },
              {
                "type": "ALIAS",
                "content": {
                  "type": "PATTERN",
                  "value": "[^|:}]+"
                },
                "named": true,
                "value": "parameter"
              }
            ]
          }
        }
      ]
    },
    "parameter": {
      "type": "PATTERN",
      "value": "[^\\s=]+[\\s]*=[\\s]*(('[^']*'|\"[^\"]*\"|\\[[^\\]]*]|[^\\s}]+))"
    },
    "text": {
      "type": "PREC",
      "value": -1,
      "content": {
        "type": "PATTERN",
        "value": "[^\\s\\|{*}-]([^\\|{*}]*[^\\|{*}-])?"
      }
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "\\s+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

